{
  "name": "nfl",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-meteor.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "deploy": "mup deploy --settings=prod-settings.json --verbose",
    "initApp": "meteor npm prune && meteor npm i",
    "predeploy": "mup setup",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor --settings settings.json",
    "debug": "meteor debug --settings settings.json",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.5",
    "date-fns": "1.29.0",
    "formik": "0.11.11",
    "jquery": "3.1.0",
    "meteor-node-stubs": "0.2.3",
    "node-sass": "4.9.3",
    "react": "15.3.0",
    "react-addons-create-fragment": "15.3.0",
    "react-addons-css-transition-group": "15.3.0",
    "react-addons-linked-state-mixin": "15.3.0",
    "react-addons-perf": "15.3.0",
    "react-addons-pure-render-mixin": "15.3.0",
    "react-addons-test-utils": "15.6.2",
    "react-addons-transition-group": "15.3.0",
    "react-addons-update": "15.3.0",
    "react-dom": "15.3.0",
    "react-helmet": "3.1.0",
    "react-inlinesvg": "0.5.3",
    "react-quill": "1.3.1",
    "react-router": "2.6.0",
    "react-sortablejs": "1.1.0",
    "recharts": "0.13.3",
    "sortablejs": "1.4.2",
    "sweetalert": "1.1.3",
    "tether": "1.3.3",
    "twilio": "3.6.2",
    "yup": "0.21.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  },
  "devDependencies": {
    "@types/meteor": "1.4.21",
    "ajv": "6.5.3",
    "autoprefixer": "^6.3.7",
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-you-dont-need-momentjs": "1.3.2",
    "postcss": "6.0.22",
    "postcss-load-config": "1.2.0"
  }
}

{
  "name": "nfl",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-meteor.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "initApp": "meteor npm i && meteor npm prune",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --settings settings.json",
    "debug": "meteor run --inspect-brk --settings settings.json",
    "debug-full": "meteor debug --inspect-brk --settings settings.json",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "dependencies": {
    "autoprefixer": "8.6.3",
    "babel-runtime": "6.26.0",
    "bcrypt": "2.0.1",
    "bootstrap": "4.1.1",
    "formik": "0.11.11",
    "jquery": "3.3.1",
    "meteor-node-stubs": "0.4.1",
    "node-sass": "4.9.0",
    "popper.js": "1.14.3",
    "postcss-load-config": "1.2.0",
    "react": "16.4.1",
    "react-addons-create-fragment": "15.6.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-addons-linked-state-mixin": "15.6.2",
    "react-addons-perf": "15.4.2",
    "react-addons-pure-render-mixin": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-addons-transition-group": "15.6.2",
    "react-addons-update": "15.6.2",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-inlinesvg": "0.8.1",
    "react-router-dom": "4.3.1",
    "react-sortablejs": "1.3.6",
    "recharts": "1.0.0-beta.10",
    "sortablejs": "1.7.0",
    "sweetalert": "2.1.0",
    "tether": "1.4.4",
    "twilio": "3.17.4",
    "yup": "0.25.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "8.2.5",
    "core-js": "2.5.7",
    "eslint": "5.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "jshint": "2.9.5"
  }
}

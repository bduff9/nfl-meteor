{
  "name": "nfl",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-meteor.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "initApp": "meteor npm i && meteor npm prune",
    "prestart": "meteor npm run initApp",
    "predebug": "meteor npm run initApp",
    "start": "meteor run --settings settings.json",
    "debug": "meteor run --inspect-brk --settings settings.json",
    "debug-full": "meteor debug --inspect-brk --settings settings.json",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "dependencies": {
    "autoprefixer": "9.1.0",
    "babel-runtime": "6.26.0",
    "bcrypt": "3.0.0",
    "bootstrap": "4.1.3",
    "formik": "1.0.2",
    "jquery": "3.3.1",
    "meteor-node-stubs": "0.4.1",
    "node-sass": "4.9.3",
    "popper.js": "1.14.4",
    "postcss": "^6.0.22",
    "postcss-load-config": "^1.2.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-helmet": "5.2.0",
    "react-inlinesvg": "0.8.1",
    "react-router-dom": "4.3.1",
    "react-sortablejs": "1.3.6",
    "react-transition-group": "^1.2.1",
    "recharts": "1.1.0",
    "sortablejs": "1.7.0",
    "sweetalert": "2.1.0",
    "tether": "1.4.4",
    "twilio": "3.19.1",
    "yup": "0.26.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@types/meteor": "1.4.16",
    "@types/react": "16.4.9",
    "ajv": "^6.0.0",
    "babel-eslint": "8.2.6",
    "core-js": "2.5.7",
    "eslint": "5.3.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "jshint": "2.9.6"
  }
}

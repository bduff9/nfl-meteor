{
  "name": "nfl",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-meteor.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "debug": "meteor debug --settings settings.json",
    "deploy": "mup deploy --settings=prod-settings.json --verbose",
    "initApp": "npx typesync && meteor npm prune && meteor npm i",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "predebug": "meteor npm run initApp",
    "predeploy": "mup setup",
    "prestart": "meteor npm run initApp",
    "start": "meteor --settings settings.json",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port=3004"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,css,scss}": [
      "prettierx --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.5.5",
    "@fortawesome/fontawesome-pro": "5.10.1",
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-brands-svg-icons": "5.10.1",
    "@fortawesome/free-regular-svg-icons": "5.10.1",
    "@fortawesome/pro-light-svg-icons": "5.10.1",
    "@fortawesome/pro-regular-svg-icons": "5.10.1",
    "@fortawesome/pro-solid-svg-icons": "5.10.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "bcrypt": "3.0.6",
    "bootstrap": "4.3.1",
    "date-fns": "<2.0.0-beta.3",
    "formik": "1.5.8",
    "jquery": "3.4.1",
    "meteor-node-stubs": "0.4.1",
    "node-sass": "4.12.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-inlinesvg": "1.1.5",
    "react-quill": "1.3.3",
    "react-router-dom": "5.0.1",
    "react-sortablejs": "1.5.1",
    "react-transition-group": "4.2.2",
    "recharts": "1.6.2",
    "sortablejs": "1.9.0",
    "sweetalert": "2.1.2",
    "tether": "1.4.7",
    "twilio": "3.33.2",
    "yup": "0.27.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "last 2 versions",
          "ie >= 9",
          "and_chr >= 2.3"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@types/autoprefixer": "9.5.0",
    "@types/bcrypt": "10.0.28-alpha",
    "@types/bootstrap": "4.3.1",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/eslint": "4.16.6",
    "@types/express": "4.17.0",
    "@types/jquery": "3.3.30",
    "@types/meteor": "1.4.29",
    "@types/node-sass": "4.11.0",
    "@types/postcss-load-config": "2.0.0",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-helmet": "5.0.8",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "@types/react-transition-group": "4.2.0",
    "@types/recharts": "1.1.20",
    "@types/simpl-schema": "0.2.2",
    "@types/sortablejs": "1.7.2",
    "@types/styled-components": "4.1.18",
    "@types/stylelint": "9.10.1",
    "@types/tether": "1.4.4",
    "@types/yup": "0.26.22",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "coveralls": "3.0.5",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.1.0",
    "eslint-import-resolver-meteor": "0.4.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettierx": "0.4.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-you-dont-need-momentjs": "1.5.0",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "postcss": "<7.0.0",
    "postcss-load-config": "<2.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "typescript": "3.5.3"
  }
}
